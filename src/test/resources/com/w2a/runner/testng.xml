<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Page Object Model">

  <!-- To generate report-ng report you need to add this listners -->
  <listeners>
  	<listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
  	<listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
  	
  	<!-- invoking test-ng listeners -->
  	<listener class-name="com.w2a.listeners.CustomListeners"></listener>
  	
  </listeners>

  <test thread-count="5" name="Login Test">
    <classes>
      <class name="com.w2a.testcases.LoginTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Create Account Test">
    <classes>
      <class name="com.w2a.testcases.CreateAccountTest"/>
    </classes>
  </test> <!-- Test -->
  
  
  
  
  
</suite> <!-- Suite -->
